programa summultdiv
   inteiro a b c d
inicio
   leia a 
   leia b 
   leia c 
   d <- a + b + c 
   escreva d 
   d <- a * b * c 
   escreva d
   d <- a div b div c
   escreva d 
   se a = b
      entao
         a <- b div c
         escreva a
      senao 
        b <- b * c 
        escreva b
   fimse
fimprograma 
